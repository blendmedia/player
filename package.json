{
  "name": "@blendmedia/fuse",
  "version": "1.0.3",
  "description": "",
  "main": "dist/blend-player.js",
  "module": "dist/blend-player.module.js",
  "private": false,
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start --browsers MyChromeHeadless",
    "start": "NODE_ENV=development rollup -c config/dev.js",
    "build": "rm -rf dist && rollup -c config/prod.js",
    "prepare": "yarn run transpile && yarn run build",
    "examples": "yarn run build && hs -p 9000 & open http://localhost:9000/examples/",
    "lint": "yarn run lint:glsl && yarn run lint:js",
    "lint:glsl": "node glsl/lint.js",
    "lint:js": "eslint src/ test/ glsl/",
    "serve": "http-server -c-1 ./",
    "preserve": "yarn run start",
    "transpile": "rm -rf es6/ && babel src/ -d es6/ --no-babelrc --presets es2015-rollup --plugins transform-runtime"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blendmedia/player.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blendmedia/player/issues"
  },
  "homepage": "https://github.com/blendmedia/player#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.26.0",
    "bowser": "^1.9.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.15.0",
    "glob": "^7.1.2",
    "http-server": "^0.11.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-notify-reporter": "^1.0.1",
    "karma-rollup-preprocessor": "^5.1.1",
    "karma-sinon": "^1.0.5",
    "mocha": "^4.1.0",
    "postcss-inline-svg": "^3.0.0",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-file-as-blob": "^1.1.2",
    "rollup-plugin-glsl": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-postcss": "^1.2.8",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^0.3.1",
    "sinon": "^4.1.5"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "debounce": "^1.1.0",
    "detect-it": "^3.0.4",
    "fscreen": "^1.0.2",
    "gl-matrix": "^2.4.0"
  }
}
